{
  "builds": [
    {
      "src": "index.php",
      "use": "vercel-php@0.7.1"
    },
    {
      "src": "api/**/*.php",
      "use": "vercel-php@0.7.1"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}